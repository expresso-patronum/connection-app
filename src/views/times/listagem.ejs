<!DOCTYPE html>
<html lang="en">
  <%- include('../includes/header') %>
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="/css/listagemEmpresa-Time.css" />
    <title>Connection</title>
  </head>
  <body>
    <br /><br />
    <div id="container">
      <h4 id="seustimes">Seus times</h4>
      <br />

      <% times.forEach(time => { %>

      <form action="/times/<%=time.id%>/agenda" method="get">
        <p><%= time.nome %> da empresa <%= time.empresa %></p>
        <br />
        <button type="submit">Ver agenda</button>
      </form>
      <br />
      <% }) %>
    </div>
    <%- include('../includes/footer') %>
  </body>
</html>
